            <origin xyz="0 0 0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
            </inertial>
        <visual>
            <origin xyz="0.001 0 -0.05" rpy="${PI} 0 ${PI}"/>
            <geometry>
                <mesh filename="package://opr_description/meshes/STL/LEGS/back_link.stl" />
            </geometry>
            <material name="blue" />
