            <origin xyz="-0.001 0 -0.05" rpy="${PI} 0 0"/>
            <geometry>
                <mesh filename="package://opr_description/meshes/STL/LEGS/front_link.stl" />
            </geometry>
        </collision>
    </link>

    <link name="left_shin_back_pitch_link">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
            </inertial>
        <visual>
            <origin xyz="0.001 0 -0.05" rpy="0 0 ${PI}"/>
            <geometry>
                <mesh filename="package://opr_description/meshes/STL/LEGS/back_link.stl" />
            </geometry>
            <material name="blue" />
        </visual>
        <collision>
            <origin xyz="0.001 0 -0.05" rpy="0 0 ${PI}"/>
            <geometry>
                <mesh filename="package://opr_description/meshes/STL/LEGS/back_link.stl" />
            </geometry>
        </collision>
    </link>

    <link name="left_independent_pitch_link">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
            </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="${PI/2} ${PI/7} ${PI}"/>
            <geometry>
                <mesh filename="package://opr_description/meshes/STL/LEGS/ankle_pitch.stl" />
            </geometry>
            <material name="grey" />
